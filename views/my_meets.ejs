<%- include('./partials/header'); %>
<% if (results.length) { %>
  <% for (var i = 0; i < results.length; i++){ %>
    <div class="card text-center m-4">
      <div class="card-header">
        Starts <%= results[i].start_datetime %>, until <%= results[i].end_datetime %>
      </div>
      <div class="card-body">
        <h3 class="card-title"><%= results[i].title %></h3>
        <p class="card-text"><%= results[i].description %></p>
        <a href="#" class="btn btn-primary">Join Now</a>
        <a href="#" class="btn btn-primary">Modify</a>
        <a href="#" class="btn btn-danger">Delete</a>
      </div>
      <div class="card-footer text-muted">
        Announced the <%= results[i].announcement_datetime %>
      </div>
    </div>
  <% } %>
<% } else { %>
<div class="alert alert-primary" role="alert">
  You don't have any future meets created.
</div>
<% } %>
</div>
<script src="/js/no_go_back.js"></script>
<%- include('./partials/footer'); %>
